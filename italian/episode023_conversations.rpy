    # l "Exactly."
    l "Esatto."

# game/episode023_conversations.rpy:202
translate italian ep023_lilly_talk_7ab3e039:

    # l "Their terror campaign has continued pretty much unabatedly."
    l "La loro campagna terroristica è continuata praticamente senza sosta."

# game/episode023_conversations.rpy:203
translate italian ep023_lilly_talk_ef07bc6d:

