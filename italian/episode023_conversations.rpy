# game/episode023_conversations.rpy:220
translate italian ep023_lilly_talk_4241539e:

    # l "No, this is something major."
    l "No, questa è una cosa importante."

# game/episode023_conversations.rpy:221
translate italian ep023_lilly_talk_83ad2999:

    # l "There have been assassination attempts on members of the Sovereignty Council before."
    l "Ci sono stati tentativi di assassinio contro membri del Consiglio di Sovranità in passato."

# game/episode023_conversations.rpy:222
translate italian ep023_lilly_talk_8e40b069:

    # l "Some were successful, others were thwarted."
    l "Alcuni hanno avuto successo, altri sono stati sventati."

# game/episode023_conversations.rpy:223
